
.flex {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.align-center {
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.justify-center {
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.justify-between {
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.justify-around {
	-webkit-justify-content: space-around;
	        justify-content: space-around;
}
.banner>image{
	height: 220rpx;
	width: 100vw;
}
.navBody{
	padding: 0 20rpx;
}
.navSty{
	padding: 20rpx;
	background-color: white;
	box-shadow: 0rpx 2rpx 10rpx 0rpx rgba(6, 0, 1, 0.2);
	-webkit-transform: translateY(-30rpx);
	        transform: translateY(-30rpx);
}
.nav_item{
	text-align: center;
	width: 60rpx;
	font-size: 26rpx;
	color: #a1a19f;
}
.nav_item>image{
	width: 40rpx;
	height: 40rpx;
	margin-bottom: 10rpx;
}
.btn{
	width: 160rpx;
	height: 160rpx;
	border-radius: 100%;
	color: white;
	background-color: #0bb27e;
	font-size: 36rpx;
	font-weight: bold;
	text-align: center;
	line-height: 160rpx;
}
.tips{
	margin-top: 10rpx;
}
.tips>view{
	font-size: 22rpx;
	padding: 5rpx 10rpx;
	background-color: #F1F1F1;
}
.houseNum{
	margin-top: 20rpx;
	font-size: 28rpx;
}
.houseNum>text{
	color: #0bb27e;
}
.time{
	margin-top: 20rpx;
	font-size: 28rpx;
}

