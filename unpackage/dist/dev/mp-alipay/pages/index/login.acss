
.bodySty>image {
	position: fixed;
	height: 60vh;
	width: 100vw;
	/* top: 0; */
	bottom: 0;
	left: 0;
	right: 0;
	z-index: -1;
}
.fromSty {
	width: 70vw;
	margin: auto;
	margin-top: 200rpx;
	padding: 40rpx;
	background-color: white;
	box-shadow: 0rpx 0rpx 10rpx 0rpx rgba(6, 0, 1, 0.3);
}
.inputSty {
	margin-top: 30rpx;
	position: relative;
	width: 100%;
}
.inputSty>input {
	width: 100%;
	border-bottom: 2rpx solid #dedede;
	font-size: 32rpx;
	padding-left: 10rpx;
	padding-bottom: 10rpx;
	padding-top: 10rpx;
	box-sizing: border-box;
}
.inputSty>.placeholder_class {
	font-size: 32rpx;
	color: #b3b3b3;
}
.getCode {
	padding: 10rpx;
	color: #ff5a00;
	font-size: 22rpx;
	position: absolute;
	top: 50%;
	-webkit-transform: translateY(-50%);
	        transform: translateY(-50%);
	right: -15rpx;
}
.agreement {
	margin-top: 30rpx;
	font-size: 24rpx;
	text-align: center;
}
.agreement>text {
	color: #ff5a00;
}
.loginBtn {
	margin-top: 30rpx;
	width: 100%;
	height: 80rpx;
	line-height: 80rpx;
	background-color: #ff5a00;
	font-size: 28rpx;
	border-radius: 20rpx;
}

